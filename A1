/**
 * @param {number[]} nums
 * @return {number[]}
 */
var smallerNumbersThanCurrent = function(nums) {
    var newNums = [];
    
    for(var i = 0;i<nums.length;i++){
        var count =0;
        for(var j = 0;j<nums.length;j++){
            if(i !=j){
              if(nums[i]>nums[j]){
                count++;
            }  
            }          
        }
        newNums.push(count);
    
    }
    return newNums;
};
